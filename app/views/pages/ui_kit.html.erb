<!-- Sidebar for md+ screens -->
<div class="flex-shrink-0 px-3 pb-3 bg-white d-none d-md-block position-fixed" style="width: 280px;">
  <ul class="list-unstyled ps-0">
  	<li class="border-top my-3"></li>  
    	<%= render 'shared/ui_kit_nav' %>
    <li class="border-top my-3"></li>    
  </ul>
</div>
<!-- Top nav for xs & sm screens -->
<div class="navbar navbar-expand-md navbar-light navbar-ui d-md-none py-2 px-3">
  <div class="container-fluid">
    <div></div>
    <!-- Hamburger menu -->
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav me-auto">
      	<%= render 'shared/ui_kit_nav' %>
      </ul>
    </div>
  </div>
</div>
<section class="main-content py-5" id="top">
  <div class="container">
    <h1 class="text-4xl font-bold mb-4">Components</h1>
    <p class="text-sm">Dolor sit eu enim qui quis elit fugiat irure in aute. Adipisicing aute cupidatat dolor laborum quis tempor consectetur anim minim aute.</p>
    <section id="alert">
      <h2 class="text-2xl">Alert</h2>
      <div class="element-preview">
        <div class="alert-jb alert-jb-info alert alert-dismissible fade show" role="alert">
          <div class="content">
            <div class="line"></div>
            <i class="icon fas fa-info-circle"></i>
            <div class="details">
              <h5>Did you know?</h5>
              <p>Here is something that you might like to know.</p>
            </div>
          </div>
          <a data-bs-dismiss="alert" aria-label="Close">
            <i class="dismiss fas fa-times"></i>
          </a>
        </div>
        <div class="alert-jb alert-jb-warning alert alert-dismissible fade show" role="alert">
          <div class="content">
            <div class="line"></div>
            <i class="icon fas fa-exclamation-circle"></i>
            <div class="details">
              <h5>Uh oh, something went wrong</h5>
              <p>Sorry! There was a problem with your request.</p>
            </div>
          </div>
          <a data-bs-dismiss="alert" aria-label="Close">
            <i class="dismiss fas fa-times"></i>
          </a>
        </div>
        <div class="alert-jb alert-jb-success alert alert-dismissible fade show" role="alert">
          <div class="content">
            <div class="line"></div>
            <i class="icon fas fa-check-circle"></i>
            <div class="details">
              <h5>Yay! Everything worked!</h5>
              <p>Congrats on the internet loading your request.</p>
            </div>
          </div>
          <a data-bs-dismiss="alert" aria-label="Close">
            <i class="dismiss fas fa-times"></i>
          </a>
        </div>
