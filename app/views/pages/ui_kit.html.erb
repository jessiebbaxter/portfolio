  font-size: 30px;
  padding: 0 20px;
}

.alert-jb-info .line {
  border-left: 5px solid #037BC3;
}

.alert-jb-info .icon {
  color: #037BC3;
}

.alert-jb-warning .line {
  border-left: 5px solid #F0A92C;
}

.alert-jb-warning .icon {
  color: #F0A92C;
}

.alert-jb-success .line {
  border-left: 5px solid #20A779;
