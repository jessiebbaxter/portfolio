.alert-jb {
  display: flex;
  margin: 10px;
  padding: 15px;
  justify-content: space-between;
  align-items: center;
  background: white;
  box-shadow: 0 0 12px rgba(0,0,0,0.12);
  border-radius: 5px;
}

.alert-jb .content {
  display: flex;
  align-items: center;
}

.alert-jb h5, .alert-jb p {
  margin-bottom: 0px;
}

.alert-jb .dismiss {
  padding-right: 10px;
}

.alert-jb p, .alert-jb .dismiss {
  color: #94959A;
}

.alert-jb .line {
  height: 60px;
  border-radius: 5px;
}

